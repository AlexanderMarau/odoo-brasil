<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_br_nfe_invoice_eletronic_form" model="ir.ui.view">
        <field name="name">view.br_nfe.invoice.eletronic.form</field>
        <field name="model">invoice.eletronic</field>
        <field name="inherit_id" ref="br_account_einvoice.sped_base_eletronic_doc_form"/>
        <field name="arch" type="xml">
            <field name="codigo_retorno" position="before">
                <field name="chave_nfe" />
                <field name="protocolo_nfe" />
                <field name="recibo_nfe" />
            </field>
            <field name="finalidade_emissao" position="after">
                <field name="ind_final" />
                <field name="ind_pres" />
                <field name="ind_dest" />
            </field>
            <field name="fiscal_position_id" position="after">
                <field name="transportation_id" />
            </field>
            <field name="valor_cofins" position="after">
                <field name="valor_icms_fcp_uf_dest" />
                <field name="valor_icms_uf_dest" />
                <field name="valor_icms_uf_remet" />
            </field>
        </field>
    </record>

</odoo>
